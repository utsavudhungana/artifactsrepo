			"type": "Microsoft.Synapse/workspaces/sparkJobDefinitions",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"targetBigDataPool": {
					"referenceName": "pool1",
					"type": "BigDataPoolReference"
				},
				"requiredSparkVersion": "3.4",
				"language": "python",
				"scanFolder": false,

							}
						},
						"source": [
							"%%pyspark\n",
							"spark.sql(\"CREATE TABLE IF NOT EXISTS database1.Newtable USING CSV LOCATION 'abfss://rootfs@devteststoragesynap.dfs.core.windows.net/query_data.csv'\")"
